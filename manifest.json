{

    "manifest_version": 2,
    "name": "Gapura Digital",
    "version": "1.0",
  
    "description": "Ekstensi firefox yang cantik",
  
    "icons": {
        "48": "gapura.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.mozilla.org/*"],
        "js": ["gapuradigital.js"]
      }
    ],

    "chrome_url_overrides": {
      "newtab": "index.html"
    },

    "browser_action": {
      "default_icon": "gapura.png",
      "default_title": "Gapura Digital"
    } 

}